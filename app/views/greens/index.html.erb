<p id="notice"><%= notice %></p>

<h1>Greens</h1>

<table>
  <thead>
    <tr>
      <th>Danceability</th>
      <th>Energy</th>
      <th>Loudness</th>
      <th>Mode</th>
      <th>Valence</th>
      <th>Tempo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @greens.each do |green| %>
      <tr>
        <td><%= green.danceability %></td>
        <td><%= green.energy %></td>
        <td><%= green.loudness %></td>
        <td><%= green.mode %></td>
        <td><%= green.valence %></td>
        <td><%= green.tempo %></td>
        <td><%= link_to 'Show', green %></td>
        <td><%= link_to 'Edit', edit_green_path(green) %></td>
        <td><%= link_to 'Destroy', green, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Green', new_green_path %>
